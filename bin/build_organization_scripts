#!/usr/bin/env zsh

cargo build --release --manifest-path organization_scripts/dir_flattener/Cargo.toml;
cargo build --release --manifest-path organization_scripts/duplicate-image-remover/Cargo.toml;
cargo build --release --manifest-path organization_scripts/file_separation/Cargo.toml;
cargo build --release --manifest-path organization_scripts/remove_duplicate_urls_from_file/Cargo.toml;

cp target/release/dir_flattener bin;
cp target/release/duplicate-image-remover bin;
cp target/release/file_separation bin;
cp target/release/remove_duplicate_urls_from_file bin;
